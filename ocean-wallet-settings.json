    "aquariusParamGetRecentDatatokens": {"sort":{"created":"desc"},"from":"{from}","query":{"bool":{"must":[{"bool":{"should":[{"query_string":{"fields":["id","publicKey.owner","dataToken","dataTokenInfo.name","dataTokenInfo.symbol","service.attributes.main.name^10","service.attributes.main.author","service.attributes.additionalInformation.description","service.attributes.additionalInformation.tags"],"phrase_slop":2,"boost":5,"minimum_should_match":"2<75%","query":""}},{"query_string":{"fields":["id","publicKey.owner","dataToken","dataTokenInfo.name","dataTokenInfo.symbol","service.attributes.main.name^10","service.attributes.main.author","service.attributes.additionalInformation.description","service.attributes.additionalInformation.tags"],"boost":5,"query":"*","lenient":true}},{"match_phrase":{"content":{"query":"","boost":10}}},{"query_string":{"query":"**","fields":["id","publicKey.owner","dataToken","dataTokenInfo.name","dataTokenInfo.symbol","service.attributes.main.name^10","service.attributes.main.author","service.attributes.additionalInformation.description","service.attributes.additionalInformation.tags"],"default_operator":"AND"}}]}},{"match":{"service.attributes.main.type":"dataset OR algorithm"}},{"match":{"service.type":"access OR compute"}},{"query_string":{"query":"chainId:{chainId}"}},{"term":{"isInPurgatory":false}}]}},"size":"{size}"},
    "aquariusParamGetPoolDatatokens": {"query":{"query_string":{"query":"( {pools} ) AND (chainId:{chainId}) AND -isInPurgatory:true","fields":["dataToken"]}},"size":"{size}"},
    "aquariusParamGetMyDatatokens": {"query":{"query_string":{"query":"(publicKey.owner:{owner}) AND (service.attributes.main.type:dataset OR algorithm) AND (service.type:access OR compute) AND (chainId:{chainId})"}},"sort":{"created":"desc"},"from":"{from}","size":"{size}"},
    "subgraphParamHighestLiquidity": {"query":"query HighestLiquidityAssets {\n  pools(where: {datatokenReserve_gte: 1}, orderBy: oceanReserve, orderDirection: desc, first: {first}) {\n    id\n    datatokenAddress\n    valueLocked\n    oceanReserve\n    __typename\n  }\n}\n","operationName":"HighestLiquidityAssets","variables":{}},
    "subgraphParamFixedPrice": {"query": "query AssetFrePrice($datatoken: String) {\n  fixedRateExchanges(orderBy: id, where: {datatoken: $datatoken}) {\n    rate\n    id\n    __typename\n  }\n}\n","operationName":"AssetFrePrice","variables":{"datatoken": "{address}"}},
    "subgraphParamPoolPrice": {"query": "query AssetPoolPrice($datatokenAddress: String) {\n  pools(where: {datatokenAddress: $datatokenAddress}) {\n    id\n    spotPrice\n    consumePrice\n    datatokenAddress\n    datatokenReserve\n    oceanReserve\n    __typename\n  }\n}\n","operationName":"AssetPoolPrice","variables":{"datatokenAddress": "{address}"}},
    "subgraphParamPoolLiquidity": {"query": "query PoolLiquidity($id: ID!, $shareId: ID) {\n  pool(id: $id) {\n    id\n    totalWeight\n    totalShares\n    swapFee\n    spotPrice\n    tokens {\n      address\n      symbol\n      isDatatoken\n      balance\n      denormWeight\n      __typename\n    }\n    shares(where: {id: $shareId}) {\n      id\n      balance\n      __typename\n    }\n    __typename\n  }\n}\n","operationName":"PoolLiquidity","variables":{"id": "{address}", "shareId": "{shareId}"}},
    "subgraphParamDatatokenPurchased": {"query":"query PreviousOrder($id: String!, $account: String!) {\n  tokenOrders(first: 1, where: {datatokenId: $id, payer: $account}, orderBy: timestamp, orderDirection: desc) {\n    timestamp\n    tx\n  }\n}\n","operationName":"PreviousOrder","variables":{"id": "{id}", "account": "{account}"}}
